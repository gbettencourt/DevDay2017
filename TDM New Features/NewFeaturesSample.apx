    <function name="ValidatePassword">
      <description />
      <parameters>
        <variable name="pwd" type="Unknown" />
      </parameters>
      <variables />
      <javascript>//validate password
if (!pwd || pwd.length &lt; 8) {
	return false;
}
return true;</javascript>
