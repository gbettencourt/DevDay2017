        <function name="CheckForPhoneColumn">
          <description>To make this demo better, a new column was added to the EMPLOYEE table</description>
          <return type="Number" />
          <parameters />
          <variables>
            <variable name="hSql" type="Sql Handle" />
            <variable name="nInd" type="Number" />
            <variable name="nCount" type="Number" />
          </variables>
          <staticvariables />
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;select count(*) INTO :nCount from syscolumns where tbname = 'EMPLOYEE' and name = 'PHONE'&quot; )">
                <code line="If SqlFetchNext( hSql, nInd )">
                  <code line="If nCount = 0">
                    <code line="If SqlPrepareAndExecute( hSql, &quot;alter table employee add phone varchar(12)&quot; )">
                      <code line="Call SqlCommit( hSql )" />
                    </code>
                  </code>
                </code>
              </code>
              <code line="Call SqlDisconnect( hSql )" />
            </code>
          </actions>
        </function>
        <function name="Test">
          <description />
          <return type="Number" />
          <parameters />
          <variables>
            <variable name="hSql" type="Sql Handle" />
            <variable name="objError" type="String" />
          </variables>
          <staticvariables />
          <actions>
            <code line="If GConnect(hSql)">
              <code line="When SqlError ">
                <code line="Set objError = 'test error'" />
              </code>
              <code line="If SqlPrepareAndExecute(hSql, &quot;a query&quot;)">
                <code line="Call SqlDisconnect(hSql)" />
              </code>
              <code line="Else ">
                <code line="Call SqlDisconnect(hSql)" />
              </code>
            </code>
          </actions>
        </function>
