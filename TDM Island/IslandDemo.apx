      <functions />
      <operations>
        <operation name="GetEmployeeDetails">
          <description />
          <parameters>
            <variable name="psID" type="String">
              <binding name="EMPLOYEE_ID" />
            </variable>
          </parameters>
          <return type="clsEmployee">
            <binding name="EMPLOYEE" />
          </return>
          <variables>
            <variable name="objEmployee" type="clsEmployee" />
            <variable name="nInd" type="Number" />
            <variable name="hSql" type="Sql Handle" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;SELECT employee_id, first_name, last_name, dept_id, date_hired, current_salary, email, phone &#xD;&#xA;INTO :objEmployee.nID, :objEmployee.sFirstName, :objEmployee.sLastName, :objEmployee.sDeptID, :objEmployee.dtDateHired, :objEmployee.nCurrentSalary, :objEmployee.sEmail, :objEmployee.sPhone &#xD;&#xA;FROM employee where employee_id = :psID&quot;)">
                <code line="Call SqlFetchNext( hSql, nInd )" />
              </code>
              <code line="Call SqlDisconnect( hSql )" />
              <code line="Return objEmployee" />
            </code>
          </actions>
        </operation>
        <operation name="SaveChanges">
          <description />
          <parameters>
            <variable name="pobjEmployee" type="clsEmployee">
              <binding name="EMPLOYEE" />
            </variable>
            <variable name="pbinPhoto" type="Binary">
              <binding name="EMPLOYEE_PHOTO" />
            </variable>
          </parameters>
          <return type="Boolean">
            <binding name="SAVESTATUS" />
          </return>
          <variables>
            <variable name="hSql" type="Sql Handle" />
            <variable name="bStatus" type="Boolean" />
            <variable name="nErrorNum" type="Number" />
            <variable name="sError" type="String" />
          </variables>
          <actions>
            <code line="Set bStatus = FALSE" />
            <code line="If GConnect( hSql )">
              <code commented="true" line="  New employee?" />
              <code line="If pobjEmployee.nID = &quot;&quot;">
                <code line="Set pobjEmployee.nID = SalStrLeftX( pobjEmployee.sFirstName, 1) || &quot;Z&quot; || SalStrLeftX( pobjEmployee.sLastName, 1)" />
                <code line="When SqlError ">
                  <code line="Set nErrorNum = SqlError( hSql )" />
                  <code line="Call SqlGetErrorText( nErrorNum, sError )" />
                </code>
                <code line="If SqlPrepareAndExecute( hSql, &quot;INSERT INTO employee(employee_id, first_name, last_name, date_hired, email, dept_id, current_salary, phone, photo)&#xD;&#xA;VALUES(:pobjEmployee.nID, :pobjEmployee.sFirstName, :pobjEmployee.sLastName, :pobjEmployee.dtDateHired, :pobjEmployee.sEmail, :pobjEmployee.sDeptID, :pobjEmployee.nCurrentSalary, :pobjEmployee.sPhone,  :pbinPhoto) &quot;)">
                  <code line="Call SqlCommit( hSql )" />
                  <code line="Set bStatus = TRUE" />
                </code>
              </code>
              <code line="Else ">
                <code line="If SqlPrepareAndExecute( hSql, &quot;UPDATE employee SET first_name = :pobjEmployee.sFirstName, &#xD;&#xA;last_name=:pobjEmployee.sLastName, &#xD;&#xA;date_hired=:pobjEmployee.dtDateHired, &#xD;&#xA;email=:pobjEmployee.sEmail, &#xD;&#xA;dept_id=:pobjEmployee.sDeptID, &#xD;&#xA;current_salary=:pobjEmployee.nCurrentSalary, &#xD;&#xA;photo=:pbinPhoto, &#xD;&#xA;phone=:pobjEmployee.sPhone  &#xD;&#xA;WHERE employee_id = :pobjEmployee.nID&quot;)">
                  <code line="Call SqlCommit( hSql )" />
                  <code line="Set bStatus = TRUE" />
                </code>
              </code>
              <code line="Call SqlDisconnect( hSql )" />
            </code>
            <code line="Return bStatus" />
          </actions>
        </operation>
        <operation name="GetEmployeePhoto">
          <description />
          <parameters>
            <variable name="psID" type="String">
              <binding name="EMPLOYEE_ID" />
            </variable>
          </parameters>
          <return type="Binary">
            <binding name="EMPLOYEE_PHOTO" />
          </return>
          <variables>
            <variable name="nInd" type="Number" />
            <variable name="hSql" type="Sql Handle" />
            <variable name="binPhoto" type="Binary" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;SELECT photo INTO :binPhoto FROM employee WHERE employee_id = :psID&quot;)">
                <code line="Call SqlFetchNext( hSql, nInd )" />
              </code>
              <code line="Call SqlDisconnect( hSql )" />
            </code>
            <code line="Return binPhoto" />
          </actions>
        </operation>
      </operations>
