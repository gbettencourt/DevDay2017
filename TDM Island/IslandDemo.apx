      <operations>
        <operation name="GetEmployeeDetails">
          <description />
          <parameters>
            <variable name="psID" type="String">
              <binding name="EMPLOYEE_ID" />
            </variable>
          </parameters>
          <return type="clsEmployee">
            <binding name="EMPLOYEE" />
          </return>
          <variables>
            <variable name="objEmployee" type="clsEmployee" />
            <variable name="nInd" type="Number" />
            <variable name="hSql" type="Sql Handle" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;SELECT first_name, last_name, dept_id, date_hired, current_salary, email, photo, phone  &#xD;&#xA;INTO :objEmployee.sFirstName, :objEmployee.sLastName, :objEmployee.sDeptID, :objEmployee.dtDateHired, :objEmployee.nCurrentSalary, :objEmployee.sEmail, :objEmployee.binPhoto, :objEmployee.sPhone &#xD;&#xA;FROM employee where employee_id = :psID&quot;)">
                <code line="Call SqlFetchNext( hSql, nInd )" />
              </code>
              <code line="Call SqlDisconnect( hSql )" />
              <code line="Return objEmployee" />
            </code>
          </actions>
        </operation>
        <operation name="DeleteEmployee">
          <description />
          <parameters>
            <variable name="psID" type="String">
              <binding name="EMPLOYEE_ID" />
            </variable>
          </parameters>
          <return type="Number" />
          <variables>
            <variable name="hSql" type="Sql Handle" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;DELETE FROM employee WHERE employee_id = :psID&quot;)">
                <code line="Call SqlCommit( hSql )" />
              </code>
              <code line="Call SqlDisconnect( hSql )" />
            </code>
          </actions>
        </operation>
      </operations>
