            <variable name="pCompId" type="Number">
              <binding name="COMPANY_ID" />
            </variable>
            <variable name="pInvoice" type="clsInvoice">
              <binding name="INVOICE" />
            </variable>
          </parameters>
          <return type="Number" />
          <variables>
            <variable name="compName" type="String" />
            <variable name="nextInvoiceNum" type="Number" />
            <variable name="nInd" type="Number" />
            <variable name="hSql" type="Sql Handle" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="Call SqlPrepareAndExecute( hSql, &quot;select max(invoice_no) INTO :nextInvoiceNum from invoice&quot;)" />
              <code line="Call SqlFetchNext( hSql, nInd )" />
              <code line="Call SqlPrepareAndExecute( hSql, &quot;select company_name INTO :compName from company where company_id = :pCompId&quot;)" />
              <code line="Call SqlFetchNext( hSql, nInd )" />
              <code line="Set nextInvoiceNum = nextInvoiceNum + 1" />
              <code line="Set pInvoice.InvoiceNo = nextInvoiceNum" />
              <code line="Call SqlPrepareAndExecute( hSql, &quot;insert into invoice (invoice_no, company_id, company_name, invoice_date, date_paid, status, amount_paid) values (:pInvoice.InvoiceNo, :pCompId, :compName, :pInvoice.InvoiceDate, :pInvoice.InvoiceDatePaid, :pInvoice.Status, :pInvoice.Amount)&quot;)" />
            </code>
          </actions>
        </operation>
        <operation name="GetInvoiceDetails">
          <description />
          <parameters>
            <variable name="pnInvoiceNo" type="Number">
              <binding name="INVOICE_NO" />
            </variable>
          </parameters>
          <return type="clsInvoice">
            <binding name="INVOICE" />
          </return>
          <variables>
            <variable name="hSql" type="Sql Handle" />
            <variable name="objInvoice" type="clsInvoice" />
            <variable name="nInd" type="Number" />
          </variables>
          <actions>
            <code line="If GConnect( hSql )">
              <code line="If SqlPrepareAndExecute( hSql, &quot;select invoice_no, invoice_date, date_paid, status, amount_paid INTO :objInvoice.InvoiceNo, :objInvoice.InvoiceDate, :objInvoice.InvoiceDatePaid, :objInvoice.Status, :objInvoice.Amount from invoice where invoice_no  = :pnInvoiceNo&quot; )">
                <code line="Call SqlFetchNext( hSql, nInd )" />
              </code>
              <code line="Call SqlDisconnect( hSql )" />
            </code>
            <code line="Return objInvoice" />
          </actions>
        </operation>
      </operations>
